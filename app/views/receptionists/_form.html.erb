<%= form_with(model: receptionist, local: true) do |form| %>
    <% if receptionist.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(receptionist.errors.count, "error") %> prohibited this receptionist from being saved:</h2>
  
        <ul>
          <% receptionist.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
        </ul>
      </div>
    <% end %>
  
    <div class="field">
      <%= form.label :name %>
      <%= form.text_field :name %>
    </div>
  
    <div class="field">
      <%= form.label :email %>
      <%= form.email_field :email %>
    </div>
  
    <div class="field">
      <%= form.label :password %>
      <%= form.password_field :password %>
    </div>
  
    <div class="field">
      <%= form.label :password_confirmation %>
      <%= form.password_field :password_confirmation %>
    </div>
  
    <div class="actions">
      <%= form.submit %>
    </div>
<% end %>